{
    "runs": {
        "replicates": 5,
        "values": [1, 2]
    },
    "files": {
        "root_dir": "example/path",
        "template_dir": "example/path/template", 
        "input_file_relative_to_template": "relative_path/input.txt",
        "input_search_term": "n_steps",
        "script_module_relative_to_template": "relative_path.script",
        "outputs": "example/path/profile_outputs"
    }, 
    "slurm":{
        "time": "48:00:00",
        "account": "swanson",
        "partition": "lonepeak",
        "nodes": 1
    },
    "conda_env": "msrkm",
    "slurm_commands": [
        "module load miniconda3",
        "conda run -n \"conda_env\" python -m cProfile -o \"{profile_name}\" -m \"{script_module}\" \"{input_file}\" >> \"{output_file}\"",
        "cp \"{profile_name}\" \"{outputs}\"",
        "cp \"{output_file}\" \"{outputs}\"",
        "rm \"{working_dir}\""
    ],
    "analyze_profiles":false
}